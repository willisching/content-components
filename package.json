{
  "name": "@brightspace/content-components",
  "description": "Web components for content in Brightspace",
  "version": "3.8.3",
  "type": "module",
  "scripts": {
    "eslint-fix": "eslint . --ext .js,.html,.cjs --fix",
    "lint": "npm run lint:eslint && npm run lint:lit",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer  \"{applications,core,capture,test,demo}/**/*.js\" --strict --rules.no-unknown-tag-name off --rules.no-unknown-attribute off",
    "start:d2l-capture-producer": "es-dev-server --app-index capture/d2l-capture-producer/demo/index.html --node-resolve --dedupe --open --watch",
    "start:d2l-content-properties": "es-dev-server --config ./core/d2l-content-properties/es-dev-server.config.cjs --app-index core/d2l-content-properties/demo/index.html --node-resolve --dedupe --open --watch",
    "start:d2l-content-selector-list": "es-dev-server --config ./core/d2l-content-selector-list/es-dev-server.config.cjs --app-index core/d2l-content-selector-list/demo/index.html --node-resolve --dedupe --open --watch",
    "start:d2l-content-topic-settings": "es-dev-server --config ./core/d2l-content-topic-settings/es-dev-server.config.cjs --app-index core/d2l-content-topic-settings/demo/index.html --node-resolve --dedupe --open --watch",
    "test": "npm run lint && npm run test:headless",
    "test:headless": "web-test-runner --group default",
    "test:headless:watch": "web-test-runner --group default --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Brightspace/content-components.git"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/eslint-parser": "^7",
    "@open-wc/testing": "^3",
    "@webcomponents/webcomponentsjs": "^2",
    "es-dev-server": "^1",
    "@web/test-runner": "^0.13",
    "@web/test-runner-playwright": "^0.8",
    "eslint": "^7",
    "eslint-config-brightspace": "^0.13",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "lit-analyzer": "^1",
    "sinon": "^9.2.4"
  },
  "dependencies": {
    "@adobe/lit-mobx": "^2",
    "@brightspace-ui-labs/accordion": "^3",
    "@brightspace-ui-labs/file-uploader": "^3",
    "@brightspace-ui-labs/media-player": "^3",
    "@brightspace-ui-labs/pagination": "^2",
    "@brightspace-ui/core": "^2",
    "@brightspace-ui/intl": "^3",
    "@chaitin/querystring": "^1.1.0",
    "@createjs/easeljs": "^2.0.0-beta.4",
    "@polymer/polymer": "^3",
    "d2l-content-service-api-client": "^7.0.2",
    "d2l-content-service-browser-http-client": "^7.0.2",
    "d2l-fetch": "github:Brightspace/d2l-fetch#semver:^2",
    "d2l-fetch-auth": "^1",
    "d2l-polymer-behaviors": "github:Brightspace/d2l-polymer-behaviors-ui#semver:^3",
    "d2l-resize-aware": "github:BrightspaceUI/resize-aware#semver:^1",
    "enum": "^3.0.4",
    "file-drop-element": "^1.0.1",
    "ismobilejs": "^1.1.1",
    "lit": "^2",
    "lit-element": "^3",
    "lit-html": "^2",
    "mobx": "^5.15.6",
    "moment": "^2.29.3",
    "page": "^1.11.6",
    "parse-srt": "^1.0.0-alpha",
    "siren-parser": "^8",
    "superagent": "^3",
    "superagent-d2l-session-auth": "^2.0.1",
    "webvtt-parser": "^2.1.2"
  }
}
